{
  "name": "midman",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "ava --verbose",
    "update-bullshit-terms": "curl https://raw.githubusercontent.com/mourner/bullshit.js/master/src/terms.js > src/bullshit/terms.ts",
    "update-prism-langs": "ts-node scripts/updatePrismLangs",
    "build:client": "rollup --config rollup.config.js",
    "dev:client": "parcel client/index.html",
    "midman": "ts-node src/index",
    "scss:dark": "node-sass src_style/dark.scss -o assets --output-style compressed",
    "scss:light": "node-sass src_style/light.scss -o assets --output-style compressed",
    "scss": "npm run scss:dark && npm run scss:light"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/marked": "^0.7.2",
    "@types/node": "^13.5.3",
    "@types/prismjs": "^1.16.0",
    "ava": "^3.5.2",
    "node-sass": "^4.13.1",
    "rollup": "^1.31.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5",
    "uglifycss": "0.0.29"
  },
  "dependencies": {
    "marked": "^0.8.0",
    "prismjs": "^1.19.0",
    "xml-string": "^2.0.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
